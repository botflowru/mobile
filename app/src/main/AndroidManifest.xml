<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.botflow">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/__2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/__2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Botflow">
        <activity android:name=".activities.BotSettingsActivity"/>
        <activity android:name=".activities.MainActivity"/>
        <activity android:name=".activities.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>